<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="gmair.machine.filter">
    <resultMap id="filter" type="finley.gmair.model.machine.Filter">
        <!--@Table gmair_machine.filter-->
        <id column="filter_id" property="filterId"/>
        <result column="name" property="name"/>
        <result column="link" property="link"/>
        <result column="remarks" property="remarks"/>
    </resultMap>

    <select id="selectLinkById" parameterType="java.util.Map" resultType="java.lang.String">
        select link
        from gmair_machine.filter
        where filter_id = #{filterId, jdbcType=VARCHAR}
    </select>
</mapper>