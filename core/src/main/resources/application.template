server.port=8008

spring.application.name=core-agent

eureka.client.healthcheck=true
eureka.client.service-url.defaultZone=http://localhost:8000/eureka/

##memcache configuration
memcached.cache.servers=127.0.0.1:11211
memcached.cache.mode=static

##code will be expired with 5 minutes
memcached.cache.expiration=300

##mongodb
spring.data.mongodb.uri=mongodb://localhost:27017/gmair_packet

##rabbitmq
spring.rabbitmq.host=localhost
spring.rabbitmq.port=5672
spring.rabbitmq.username=guest
spring.rabbitmq.password=guest


# Redis数据库索引（默认为0）
spring.redis.database=0
# Redis服务器地址
spring.redis.host=localhost
# Redis服务器连接端口
spring.redis.port=6379
# Redis服务器连接密码（默认为空）
spring.redis.password=
# 连接池最大连接数（使用负值表示没有限制）
spring.redis.pool.max-active=8
# 连接池最大阻塞等待时间（使用负值表示没有限制）
spring.redis.pool.max-wait=-1
# 连接池中的最大空闲连接
spring.redis.pool.max-idle=8
# 连接池中的最小空闲连接
spring.redis.pool.min-idle=0
# 连接超时时间（毫秒）
spring.redis.timeout=0









