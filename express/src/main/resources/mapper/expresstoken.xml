<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="gmair.express.accessToken">
    <resultMap id="tokenVo" type="finley.gmair.model.express.AccessToken">
        <result property="accessToken" column="access_token"></result>
        <result property="blockFlag" column="block_flag"></result>
        <result property="createAt" column="create_time"></result>
    </resultMap>

    <select id="query" parameterType="java.util.Map" resultMap="tokenVo">
        select access_token, block_flag, create_time
        from gmair_express.express_token
        where 1 = 1
        <if test="accessToken != null">
            access_token = #{accessToken}
        </if>
        <if test="blockFlag != null">
            block_flag = #{blockFlag}
        </if>
        order by create_time desc
    </select>

</mapper>