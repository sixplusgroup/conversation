<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="gmair.bill.info">
    <resultMap id="infoVo" type="finley.gmair.model.bill.BillInfo">
        <result property="billId" column="bill_id"></result>
        <result property="orderId" column="order_id"></result>
        <result property="orderPrice" column="order_price"></result>
        <result property="actualPrice" column="actual_price"></result>
        <result property="status" column="bill_status"
                typeHandler="finley.gmair.mybatis.handler.BillStatusHandler"></result>
        <result property="blockFlag" column="block_flag"></result>
        <result property="createAt" column="create_time"></result>
    </resultMap>

    <select id="query" parameterType="java.util.Map" resultMap="infoVo">

    </select>

    <insert id="insert" parameterType="finley.gmair.model.bill.BillInfo">
        insert into gmair_bill.bill_info(bill_id, order_id, order_price, actual_price, bill_status, block_flag, create_time)
        values (#{billId}, #{orderId}, #{orderPrice}, #{actualPrice}, #{status, typeHandler=org.apache.ibatis.type.EnumOrdinalTypeHandler}, #{blockFlag}, #{createAt})
    </insert>
    
</mapper>