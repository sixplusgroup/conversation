<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="gmair.order">
    <resultMap id="orderVo" type="gmair.finley.model.GmairOrder">
        <result property="orderId" column="order_id"></result>
        <result property="name" column="receiver_name"></result>
        <result property="province" column="receiver_province"></result>
        <result property="city" column="receiver_city"></result>
        <result property="district" column="receiver_district"></result>
        <result property="address" column="receiver_address"></result>
        <result property="phone" column="receiver_phone"></result>
    </resultMap>

    <insert id="insert" parameterType="gmair.finley.model.GmairOrder">
        INSERT INTO gmair_order.gmorder (order_id, receiver_name, receiver_province, receiver_city, receiver_district, receiver_address, receiver_phone, block_flag, create_time)
        VALUES (#{orderId}, #{name}, #{province}, #{city}, #{district}, #{address}, #{phone}, #{blockFlag}, #{createAt})
    </insert>
</mapper>