<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="finley.gmair.dao.SessionMessageDOMapper">
    <insert id="updateSentimentAnalysis" parameterType="java.util.Map">
        <foreach collection="idAnalysis.entrySet()" item="v" index="k">
            update session_message_jd set analysis=#{v} where message_id=#{k}
        </foreach>

    </insert>

</mapper>