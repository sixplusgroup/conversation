<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="finley.gmair.dao.JdWaiterDOMapper">
    <select id="getWaiterIdByName" parameterType="java.lang.String" resultType="java.lang.Integer">
        SELECT waiter_id as id
        from waiter_jd
        where waiter_name = #{name}
    </select>

</mapper>