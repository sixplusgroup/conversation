server:
  port: 8008

spring:
  application:
    name: mqtt-management-agent
  mqtt:
    inbound:
      url: tcp://127.0.0.1:61613
      clientId: core-server
      topics: topic1,topic2
    outbound:
      urls: tcp://127.0.0.1:61613
      clientId: core-client
      topic: topic1
  datasource:
    driver-class-name: com.mysql.jdbc.Driver
    url: jdbc:mysql://localhost:3306/gmair_mqtt_management?useLegacyDatetimeCode=false&serverTimezone=CTT&useUnicode=true&useSSL=false&characterEncoding=UTF-8
    username: root
    password:
  redis:
    database: 0
    host: localhost
    port: 6379
    password:
    timeout: 0
    pool:
      max-active: 8
      max-wait: -1
      max-idle: 8
      min-idle: 0

mybatis:
  config-location: classpath:mybatis-config.xml
eureka:
  client:
    healthcheck: true
    service-url:
      defaultZone: http://localhost:8000/eureka/

