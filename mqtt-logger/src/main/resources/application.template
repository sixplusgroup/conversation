server:
  port: 8011

spring:
  application:
    name: mqtt-logger
  mqtt:
    inbound:
      url: tcp://172:1883
      clientId: logger-client
      topics: /client/FA/+/update,/client/FA/+/rfid_enabled
    outbound:
      urls: tcp://192.168.0.102:1883
      clientId: logger-client
      topic: topic1
  datasource:
    driver-class-name: com.mysql.jdbc.Driver
    url: jdbc:mysql://localhost:3306/gmair_mqtt?useLegacyDatetimeCode=false&serverTimezone=CTT&useUnicode=true&useSSL=false&characterEncoding=UTF-8
    username: root
    password: root
mybatis:
    config-location: classpath:mybatis-config.xml


eureka:
  client:
    healthcheck: true
    service-url:
      defaultZone: http://localhost:8000/eureka/

logging:
  level:
    mapper: debug

